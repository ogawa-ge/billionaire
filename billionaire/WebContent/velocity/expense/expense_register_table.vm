<table>
	<tr>
		<th>出費名</th>
		<td>#springFormInput("expense.expenseName.value")</td>
		<th>*出費カテゴリ</th>
		<td>#springFormSingleSelect("expense.expenseCategory" $expense.expenseCategory.options() "" "")</td>
	</tr>
	<tr>
		<th>*出費額</th>
		<td>#springFormInput("expense.expenseAmount.value") 円</td>
	</tr>
	<tr>
		<td colspan="2">#springShowErrors("<br>" "color : red")</td>
	</tr>
	<tr>
		<th>備考</th>
		<td>#springFormInput("expense.expenseMemo.value")</td>
	</tr>
</table>
